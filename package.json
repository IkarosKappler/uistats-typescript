{
  "name": "uistats-typescript",
  "version": "0.0.1",
  "description": "A tiny ui component to display app stats.",
  "main": "dist/uistats.js",
  "scripts": {
    "build": "npm run compile-typescript && rollup -c --input ./src/js/index.js && npm run webpack",
    "compile-typescript": "tsc",
    "test": "",
    "watch": "rollup -cw",
    "start": "npm run watch & serve",
    "webpack": "npx webpack --mode production --config ./webpack5-config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IkarosKappler/uistats-typescript"
  },
  "keywords": [
    "ui",
    "stats"
  ],
  "author": "Ikaros Kappler <info@int2byte.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IkarosKappler/uistats-typescript/issues"
  },
  "homepage": "https://github.com/IkarosKappler/uistats-typescript",
  "devDependencies": {
    "catw": "^1.0.1",
    "leaflet": "^0.7.7",
    "leaflet-draw": "^0.2.3",
    "reify": "^0.17.3",
    "rollup": "^0.65.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^2.0.2",
    "serve": "^10.0.0",
    "typescript": "^4.1.3",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@types/eslint": "^7.2.6"
  }
}
