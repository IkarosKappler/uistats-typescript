  "description": "A tiny ui component to display app stats.",
  "main": "dist/uistats.js",
  "scripts": {
    "rollup": "rollup --config rollup.config.js",
    "build": "npm run compile-typescript && npm run rollup && npm run webpack",
    "build-dev": "npm run compile-typescript && npm run rollup",
    "compile-typescript": "tsc",
