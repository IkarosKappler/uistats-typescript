  },
  "homepage": "https://github.com/IkarosKappler/uistats-typescript",
  "devDependencies": {
    "catw": "^1.0.1",
    "reify": "^0.20.12",
    "rollup": "^2.35.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
